<% include header %>

<div class="container">
	<div class="main-container">
		<h1>Movies page</h1>
		<table class="table table-striped" id="moviesTable">
			<thead>
				<tr>
					<th>Title</th>
					<th>Released</th>
				</tr>
			</thead>

			<tbody>
					<% for (var i = 0; i < movies.length; i++) { %>
						<tr>
							<td><a href="/movies/<%= movies[i].data._id %>" ><%= movies[i].data.properties.title %></a></td>
						 	<td><%= movies[i].data.properties.released %></td>
					 	<tr>
					<% } %>
			</tbody>
		</table>
	</div>
</div>

<% include footer %>